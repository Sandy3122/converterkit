{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/public/imgToPdf/convert", "dest": "/public/imgToPdf/convert" },
    { "src": "/public/imgToPdf/download/([^/]+)", "dest": "/public/imgToPdf/download/$1" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
